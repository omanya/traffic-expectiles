@article{zheng_fangfang2017,
author = {Zheng, Fangfang and van Zuylen, Henk and Liu, Xiaobo},
title = {A Methodological Framework of Travel Time Distribution Estimation for Urban Signalized Arterial Roads},
journal = {Transportation Science},
volume = {51},
number = {3},
pages = {893-917},
year = {2017},
doi = {10.1287/trsc.2016.0718},

URL = { 
    
